<header>
    {{template "views/layouts/menu" .}}
</header>
<section class="container">
    <div class="box"></div>
    <h1>{{.Data.Title}}</h1>
    <div id="dictionaries">
        <ul>
            {{range .Data.Dictionaries}}
                <li>
                    <a href="/{{.Slug}}">
                        {{.Title}}
                    </a>
                </li>
            {{end}}
        </ul>
    </div>
</section>
{{template "views/layouts/footer" .}}